          "name": "Update product",
          "request": {
            "method": "PUT",
            "url": {
              "raw": "{{base_url}}/api/v1/products/{{product_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "products", "{{product_id}}"]
            },
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"attribute_values\": [1, 2, 3]\n}"
            },
            "description": "Update an existing product with attribute values"
          },
          "response": []
        },
