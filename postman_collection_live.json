          "name": "Delete product",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{base_url}}/api/v1/products/{{product_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "products", "{{product_id}}"]
            },
            "description": "Delete a product (if it doesn't have any associated pricing)"
          },
          "response": []
        }
